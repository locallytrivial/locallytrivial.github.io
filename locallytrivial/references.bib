@misc{rayWhenPointYour2023,
  title = {When to {{Point Your Telescopes}}: {{Gravitational Wave Trigger Classification}} for {{Real-Time Multi-Messenger Followup Observations}}},
  shorttitle = {When to {{Point Your Telescopes}}},
  author = {Ray, Anarya and Niu, Wanting and Sakon, Shio and Ewing, Becca and Creighton, Jolien D. E. and Hanna, Chad and Adhicary, Shomik and Baral, Pratyusava and Baylor, Amanda and Cannon, Kipp and Caudill, Sarah and Cousins, Bryce and Fong, Heather and George, Richard N. and Godwin, Patrick and Harada, Reiko and Huang, Yun-Jing and Huxford, Rachael and Joshi, Prathamesh and Kapadia, Shasvath and Kennington, James and Kuwahara, Soichiro and Li, Alvin K. Y. and Magee, Ryan and Meacher, Duncan and Messick, Cody and Morisaki, Soichiro and Mukherjee, Debnandini and Pace, Alex and Posnansky, Cort and Sachdev, Surabhi and Singh, Divya and Tapia, Ron and Tsukada, Leo and Tsutsui, Takuya and Ueno, Koh and Viets, Aaron and Wade, Leslie and Wade, Madeline},
  year = 2023,
  month = oct,
  number = {arXiv:2306.07190},
  eprint = {2306.07190},
  primaryclass = {astro-ph, physics:gr-qc},
  publisher = {arXiv},
  doi = {10.48550/arXiv.2306.07190},
  urldate = {2024-03-19},
  abstract = {We develop a robust and self-consistent framework to extract and classify gravitational wave candidates from noisy data, for the purpose of assisting in real-time multi-messenger follow-ups during LIGO-Virgo-KAGRA's fourth observing run\textasciitilde (O4). Our formalism implements several improvements to the low latency calculation of the probability of astrophysical origin\textasciitilde (\textbackslash PASTRO\textbraceleft\textbraceright ), so as to correctly account for various factors such as the sensitivity change between observing runs, and the deviation of the recovered template waveform from the true gravitational wave signal that can strongly bias said calculation. We demonstrate the high accuracy with which our new formalism recovers and classifies gravitational wave triggers, by analyzing replay data from previous observing runs injected with simulated sources of different categories. We show that these improvements enable the correct identification of the majority of simulated sources, many of which would have otherwise been misclassified. We carry out the aforementioned analysis by implementing our formalism through the \textbackslash GSTLAL\textbraceleft\textbraceright{} search pipeline even though it can be used in conjunction with potentially any matched filtering pipeline. Armed with robust and self-consistent \textbackslash PASTRO\textbraceleft\textbraceright{} values, the \textbackslash GSTLAL\textbraceleft\textbraceright{} pipeline can be expected to provide accurate source classification information for assisting in multi-messenger follow-up observations to gravitational wave alerts sent out during O4.},
  archiveprefix = {arXiv},
  keywords = {Astrophysics - High Energy Astrophysical Phenomena,General Relativity and Quantum Cosmology},
  file = {/Users/jim/Zotero/storage/TLS6ILPJ/Ray et al. - 2023 - When to Point Your Telescopes Gravitational Wave .pdf;/Users/jim/Zotero/storage/4IF2NF24/2306.html}
}

