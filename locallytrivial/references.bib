@article{tsukadaImprovedRankingStatistics2023,
  title = {Improved Ranking Statistics of the {{GstLAL}} Inspiral Search for Compact Binary Coalescences},
  author = {Tsukada, Leo and Joshi, Prathamesh and Adhicary, Shomik and George, Richard and Guimaraes, Andre and Hanna, Chad and Magee, Ryan and Zimmerman, Aaron and Baral, Pratyusava and Baylor, Amanda and Cannon, Kipp and Caudill, Sarah and Cousins, Bryce and Creighton, Jolien D. E. and Ewing, Becca and Fong, Heather and Godwin, Patrick and Harada, Reiko and Huang, Yun-Jing and Huxford, Rachael and Kennington, James and Kuwahara, Soichiro and Li, Alvin K. Y. and Meacher, Duncan and Messick, Cody and Morisaki, Soichiro and Mukherjee, Debnandini and Niu, Wanting and Pace, Alex and Posnansky, Cort and Ray, Anarya and Sachdev, Surabhi and Sakon, Shio and Singh, Divya and Tapia, Ron and Tsutsui, Takuya and Ueno, Koh and Viets, Aaron and Wade, Leslie and Wade, Madeline},
  year = 2023,
  month = aug,
  journal = {Physical Review D},
  volume = {108},
  number = {4},
  eprint = {2305.06286},
  primaryclass = {astro-ph, physics:gr-qc},
  pages = {043004},
  issn = {2470-0010, 2470-0029},
  doi = {10.1103/PhysRevD.108.043004},
  urldate = {2024-03-19},
  abstract = {Starting from May 2023, the LIGO Scientific, Virgo and KAGRA Collaboration is planning to conduct the fourth observing run with improved detector sensitivities and an expanded detector network including KAGRA. Accordingly, it is vital to optimize the detection algorithm of low-latency search pipelines, increasing their sensitivities to gravitational waves from compact binary coalescences. In this work, we discuss several new features developed for ranking statistics of GstLAL-based inspiral pipeline, which mainly consist of: the signal contamination removal, the bank-\$\textbackslash xi\textasciicircum 2\$ incorporation, the upgraded \$\textbackslash rho-\textbackslash xi\textasciicircum 2\$ signal model and the integration of KAGRA. An injection study demonstrates that these new features improve the pipeline's sensitivity by approximately 15\% to 20\%, paving the way to further multi-messenger observations during the upcoming observing run.},
  archiveprefix = {arXiv},
  keywords = {Astrophysics - Instrumentation and Methods for Astrophysics,General Relativity and Quantum Cosmology},
  file = {/Users/jim/Zotero/storage/GY9DYZ9T/Tsukada et al. - 2023 - Improved ranking statistics of the GstLAL inspiral.pdf;/Users/jim/Zotero/storage/UJQGAJ37/2305.html}
}

